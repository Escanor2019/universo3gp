		<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.2/owl.carousel.min.js"></script>
		<script>
			$(document).ready(function () {

				$('.btn-categories').on('click', function () {
					var $target = $(this).data('target'); //capturar o target do btn
					$('.multi-collapse').addClass('d-none'); //deixa tudo invisível
					$('.btn-categories').removeClass('active border border-info'); //clean btn selected
					$(this).addClass('active border border-info'); //this btn is selected
					$('.card').fadeIn('slow').removeClass('d-none'); //clean filter
					$('.multi-collapse[data-status="' + $target + '"]').fadeIn('slow').removeClass('d-none'); //select filter
				});
				$('.btn-filter').on('click', function () {
					var $target = $(this).data('target');
					if ($target != 'all') {
						$('.card').addClass('d-none');
						$('.btn-filter').removeClass('active btn-danger disabled');
						$(this).addClass('active btn-danger disabled');
						$('.card[data-status="' + $target + '"]').fadeIn('slow').removeClass('d-none');
					} else {
						$('.btn-filter').removeClass('active btn-danger disabled');
						$(this).addClass('active btn-danger disabled');
						$('.card').fadeIn('slow').removeClass('d-none');
					}
				});

			});
		</script> 

