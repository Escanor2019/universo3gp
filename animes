		<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
		<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.2/owl.carousel.min.js"></script>

		<script>
			$(document).ready(function () {

				if(window.location.hash) {
					var hash = window.location.hash;
					hash_class(hash);
				}

				function hash_class(hash_page) {

					$('.list-search .card-body a').addClass('d-none');
					$('#data-search').addClass('d-none');

					switch(hash_page) {
						case "#c-livre":
							var $target = "c-livre";
							var $indicativa = "Classificação Indicativa Livre";
							break;
						case "#c-10":
							var $target = "c-10";
							var $indicativa = "Classificação Indicativa 10 anos";
							break;
						case "#c-12":
							var $target = "c-12";
							var $indicativa = "Classificação Indicativa 12 anos";
							break;
						case "#c-14":
							var $target = "c-14";
							var $indicativa = "Classificação Indicativa 14 anos";
							break;
						case "#c-16":
							var $target = "c-16";
							var $indicativa = "Classificação Indicativa 16 anos";
							break;
						case "#c-18":
							var $target = "c-18";
							var $indicativa = "Classificação Indicativa 18 anos";
							break;
					}

					$('.list-search .card-body a[data-status="' + $target + '"]').removeClass('d-none');
					$('.search-query-sf').remove();
					$('#c-indicativa').empty();
					$('#c-indicativa').removeClass('d-none');
					$('#c-indicativa').prepend($indicativa);
					empty_card();
				}

				$('.btn-filter').on('click', function () {
					var $target = $(this).data('target');
					if ($target != 'all') {
						$('.card').addClass('d-none');
						$('.btn-filter').removeClass('active btn-danger disabled');
						$(this).addClass('active btn-danger disabled');
						$('#data-search').addClass('d-none');
						$('.card[data-status="' + $target + '"]').fadeIn('slow').removeClass('d-none');
					} else {
						$('.btn-filter').removeClass('active btn-danger disabled');
						$(this).addClass('active btn-danger disabled');
						$('.card').fadeIn('slow').removeClass('d-none');
						$('#data-search').addClass('d-none');
					}
				});

				$('#system-search').keyup( function() {
					var that = this;
					// affect all table rows on in systems table
					var tableBody = $('#push');
					var tableRowsClass = $('.list-search .card-body a');
					$('.search-sf').remove();

					//$('.list-search').addClass('d-none');
					//$('.list-search[data-status="' + x.charAt(0) + '"]').fadeIn('slow').removeClass('d-none');
					var clone = [];
					tableRowsClass.each( function(i, val) {

						//Lower text for case insensitive
						var rowText = $(val).text().toLowerCase();
						var inputText = $(that).val().toLowerCase();
						if(inputText != '')
						{
							$('.search-query-sf').remove();
							$('#push').empty();
							$('#push').removeClass('d-none');
							tableBody.prepend('Pesquisando por: "' + $(that).val() + "'");
						}
						else
						{												
							$('.search-query-sf').remove();
							$('#data-search').addClass('d-none');
						}

						if( rowText.indexOf( inputText ) == -1 )
						{
							//hide rows
							tableRowsClass.eq(i).hide();

						}
						else
						{
							$('.search-sf').remove();
							tableRowsClass.eq(i).show();
							clone[clone.length] = tableRowsClass.eq(i).clone(); //tableRowsClass.eq(i).clone();

						}
					});

					if($(that).val().toLowerCase() != '')
					{
						$('.list-search').addClass('d-none');
						$('#filter').addClass('d-none');
						$('#data-search').removeClass('d-none');
						document.getElementById("search-results").innerHTML=""; 
						$('#search-results').empty();
						$('#search-results').append(clone);
					}
					else
					{
						$('.list-search').removeClass('d-none');
						$('#filter').removeClass('d-none');
						$('#filter-no').click();
						$('#data-search').addClass('d-none');
					}

					//all tr elements are hidden
					if(tableRowsClass.children(':visible').length == 0 && clone == '')
					{
						$('#push').removeClass('d-none');
						$('#push').empty();
						tableBody.append('Nenhum Resultado Encontrado');
						$('#search-results').addClass('d-none');
					}
				});
			});
		</script>


	</body></html>
